<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font android:font="@font/inter_light" android:fontWeight="300" android:fontStyle="normal"
        app:font="@font/inter_light" app:fontWeight="300" app:fontStyle="normal"
        tools:targetApi="o" />

    <font android:font="@font/inter_regular" android:fontWeight="400" android:fontStyle="normal"
        app:font="@font/inter_regular" app:fontWeight="400" app:fontStyle="normal"
        tools:targetApi="o" />

    <font android:font="@font/inter_medium" android:fontWeight="500" android:fontStyle="normal"
        app:font="@font/inter_medium" app:fontWeight="500" app:fontStyle="normal"
        tools:targetApi="o" />

    <font android:font="@font/inter_bold" android:fontWeight="700"
        app:font="@font/inter_bold" app:fontWeight="700"
        tools:targetApi="o" />

</font-family>